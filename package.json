{
  "private": true,
  "name": "coding-garden",
  "description": "Thilo's digital garden for full-stack web development.",
  "version": "0.0.0",
  "author": "Thilo Maier <git@maier.asia>",
  "homepage": "https://coding.maier.tech",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "yarn run lint:eslint && yarn run lint:prettier",
    "lint:eslint": "eslint . --ignore-path .gitignore",
    "lint:prettier": "yarn run prettier --check",
    "prettier": "prettier . --ignore-path .gitignore",
    "format": "yarn run format:eslint && yarn run format:prettier",
    "format:eslint": "yarn run lint:eslint --fix",
    "format:prettier": "yarn run prettier --write",
    "prepare": "husky install",
    "purge": "rm -rf ./node_modules"
  },
  "dependencies": {
    "@maiertech/gatsby-theme-digital-garden": "^0.13.0",
    "gatsby": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@maiertech/eslint-config": "^2.0.1",
    "@maiertech/prettier-config": "^1.0.1",
    "eslint": "^7.26.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0"
  }
}
