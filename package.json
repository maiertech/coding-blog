{
  "private": true,
  "name": "coding-blog",
  "description": "My digital garden for web development.",
  "version": "0.0.0",
  "author": "Thilo Maier <git@maier.asia>",
  "homepage": "https://coding.maier.dev",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --ignore-path .gitignore",
    "lint:prettier": "yarn run prettier --check",
    "prettier": "prettier . --ignore-path .gitignore",
    "format": "run-s format:*",
    "format:eslint": "yarn run lint:eslint --fix",
    "format:prettier": "yarn run prettier --write",
    "purge": "rm -rf ./node_modules"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run lint"
    }
  },
  "dependencies": {
    "gatsby": "2.24.53",
    "gatsby-theme-blog": "2.0.1",
    "gatsby-theme-notes": "2.0.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "theme-ui": "0.3.1"
  },
  "devDependencies": {
    "@maiertech/eslint-config": "0.13.0",
    "@maiertech/prettier-config": "0.2.1",
    "eslint": "6.8.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.13",
    "npm-run-all": "4.1.5"
  }
}
